{
    "version": 8,
    "name": "OSM Raster",
    "sources": {
        "osm-raster": {
            "type": "raster",
            "tiles": [
                "https://tile.openstreetmap.org/{z}/{x}/{y}.png"
            ],
            "tileSize": 256,
            "attribution": "&copy; <a href=\"https://www.openstreetmap.org/copyright\">OpenStreetMap</a> contributors"
        }
    },
    "layers": [
        {
            "id": "osm-raster-layer",
            "type": "raster",
            "source": "osm-raster",
            "minzoom": 0,
            "maxzoom": 19
        }
    ]
}